{
  "mcpServers": {
    "manus-mcp": {
      "command": "wsl",
      "args": [
        "--distribution",
        "Debian",
        "--",
        "bash",
        "-c",
        "source /path/to/manus-mcp/.venv/bin/activate && python3 /path/to/manus-mcp/mcp_server.py"
      ],
      "name": "Manus MCP",
      "description": "this plugin provides the following tools: browse_web, code_interpreter, and bash_tool",
      "disabled": false,
      "timeout": 60,
      "transportType": "stdio"
    }
  }
}
